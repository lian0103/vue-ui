    "sass-loader": "^12.0.0",
    "svg-sprite-loader": "^6.0.11",
    "tailwindcss": "^3.1.8",
    "testing-library": "^0.0.2",
    "unplugin-auto-import": "^0.7.1",
    "uppercamelcase": "^3.0.0"
