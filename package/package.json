  "scripts": {
    "gen": "npm run gen:google-fonts && npm run gen:css-index && npm run gen:pkg-index && npm run gen:svg2css",
    "gen:component": "node scripts/gen-component.cjs",
    "gen:svg2css": "gulp build --gulpfile scripts/gen-svg2font.cjs",
    "gen:google-fonts": "gulp build --gulpfile scripts/gen-google-fonts.cjs",
    "gen:pkg-index": "node scripts/gen-pkg-index.cjs",
    "gen:css-index": "node scripts/gen-css-index.cjs",
    "build": "npm run build:theme && npm run build:pkg",
    "build:theme": "gulp build --gulpfile scripts/build-theme.cjs",
    "build:pkg": "gulp build --gulpfile scripts/build-pkg.cjs",
    "pub": "sh ./scripts/publish.sh"
  },
