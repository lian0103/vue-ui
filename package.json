    "uppercamelcase": "^3.0.0",
    "vite": "^2.9.7",
    "vite-plugin-md": "^0.20.2",
    "vite-svg-loader": "^3.3.0",
    "vitest": "^0.16.0",
    "vue": "^3.2.25",
    "vue-router": "^4.0.13",
    "vue-types": "^4.1.1"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  }
}
