{
  "name": "@greattree/front-ui",
  "author": {
    "name": "Jason Lien",
    "email": "k1319900103@gmail.com"
  },
  "version": "0.0.99",
  "files": [
    "dist"
  ],
  "main": "./dist/GT/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/GT/gt-ui.es.js",
      "require": "./dist/GT/gt-ui.umd.js"
    },
    "./dist/GT/style.css": "./dist/GT/style.css"
  },
  "scripts": {
    "dev": "vite --config ./build/vite.dev.config.js",
    "build-GT": "vite build --config ./build/vite.libGT.config.js",
    "build-Ele": "vite build --mode Element --config ./build/vite.libQuaEle.config.js",
    "build-Qua": "vite build --mode Quasar --config ./build/vite.libQuaEle.config.js",
    "test:unit": "vitest --config ./build/vite.dev.config.js --environment jsdom --coverage",
    "doc": "vite build --config ./build/vite.doc.config.js",
    "gen:gtCom": "node ./script/genNewComp/index.js",
    "gen:gtDoc":"node ./script/genGtDoc/index.js",

    "gen": "npm run gen:google-fonts && npm run gen:css-index && npm run gen:pkg-index && npm run gen:svg2css",
    "gen:component": "node build/bin/gen-component.cjs",
    "gen:svg2css": "gulp build --gulpfile build/bin/gen-svg2font.cjs",
    "gen:google-fonts": "gulp build --gulpfile build/bin/gen-google-fonts.cjs",
    "gen:pkg-index": "node build/bin/gen-pkg-index.cjs",
    "gen:css-index": "node build/bin/gen-css-index.cjs",
    "gen:pkgDoc":"node ./script/genGtPkgDoc/index.js",
    "build": "npm run build:theme && npm run build:pkg",
    "build:theme": "gulp build --gulpfile build/bin/build-theme.cjs",
    "build:pkg": "gulp build --gulpfile build/bin/build-pkg.cjs"
  },
  "dependencies": {
    "dayjs": "^1.11.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "@kangc/v-md-editor": "^2.3.15",
    "@quasar/extras": "^1.13.6",
    "@quasar/vite-plugin": "^1.0.9",
    "@testing-library/vue": "^6.6.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vueuse/core": "^8.5.0",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "element-plus": "^2.1.11",
    "gsap": "^3.10.4",
    "gt-front-ui": "^0.0.14",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.13",
    "postcss-import": "^14.1.0",
    "postcss-nesting": "^10.1.10",
    "quasar": "^2.6.6",
    "sass": "^1.52.2",
    "sass-loader": "^12.0.0",
    "svg-sprite-loader": "^6.0.11",
    "tailwindcss": "^3.0.24",
    "testing-library": "^0.0.2",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-element-plus": "^0.4.0",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.7",
    "vite-plugin-md": "^0.20.2",
    "vite-svg-loader": "^3.3.0",
    "vitest": "^0.16.0",
    "vue": "^3.2.25",
    "vue-router": "^4.0.13",
    "vue-types": "^4.1.1",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.0",
    "async": "^3.2.4",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-iconfont": "^11.0.1",
    "gulp-iconfont-css": "^3.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "json-templater": "^1.2.0",
    "lodash.defaults": "^4.2.0",
    "uppercamelcase": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "homepage": "https://lian0103.github.io/vue-ui/#/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lian0103/vue-ui"
  },
  "keywords": [
    "vue3",
    "UI",
    "Great Tree"
  ],
  "contributors": [
    {
      "name": "scp07602x"
    }
  ]
}
